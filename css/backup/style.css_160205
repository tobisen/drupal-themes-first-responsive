.slogan {
	background-color:rgba(0,0,0,0.4);
	padding-bottom: 20px;
	margin-top: 80px;
	-moz-border-radius: 150px;    /* Firefox 3.6-, removed in Firefox 13 */
	-webkit-border-radius: 150px; /* Safari 4-, Chrome 3- */
	border-radius: 150px;     /* Firefox 4+, Safari 5+, Chrome 4+, Opera 10.5+, IE9+ */
	border: 1px solid grey !important;
	font-weight: bold;
  	color: #FFF;
}


